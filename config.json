{
    "server": {
        "port": { "env": "JWT_USER_SERVICE_PORT", "default": 8080 }
    },
    "admin": {
        "username": { "env": "JWT_USER_SERVICE_ADMIN_USERNAME", "default": "admin" },
        "password": { "env": "JWT_USER_SERVICE_ADMIN_PASSWORD", "default": "changeit" }
    },
    "jwt": {
        "secret": { "env": "JWT_USER_SERVICE_JWT_SECRET", "default": "change it" },
        "expire": { "env": "JWT_USER_SERVICE_JWT_EXPIRE", "default": 3600 },
        "privateKey": { "env": "JWT_USER_SERVICE_JWT_PRIVATE_KEY", "default": null },
        "publicKey": { "env": "JWT_USER_SERVICE_JWT_PUBLIC_KEY", "default": null }
    },
    "storage-driver": { "env": "JWT_USER_SERVICE_STORAGE_DRIVER", "default": "./lib/database/fs" },
    "storage": {
        "filename": { "env": "JWT_USER_SERVICE_STORAGE_FILENAME", "default": "./app-data/users.db" }
    },
    "crypt-module": { "env": "JWT_USER_SERVICE_CRYPT_MODULE", "default": "./lib/crypt" },
    "crypt": {
        "salt": { "env": "JWT_USER_SERVICE_CRYPT_SALT", "default": "change this, too" }
    }
}